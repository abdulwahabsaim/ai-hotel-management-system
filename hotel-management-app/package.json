{
  "name": "hotel-management-app",
  "version": "1.0.0",
  "description": "AI Hotel Management System",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && node server.js",
    "build": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --minify",
    "tailwind:watch": "tailwindcss -i ./public/css/input.css -o ./public/css/output.css --watch",
    "dev": "concurrently \"nodemon server.js\" \"npm run tailwind:watch\"",
    "seed": "node seed.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^3.0.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "method-override": "^3.0.0",
    "mongoose": "^7.3.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  }
}